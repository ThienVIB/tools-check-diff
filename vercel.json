{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/render",
      "dest": "/api/render.js"
    },
    {
      "src": "/api/proxy",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/render.js": {
      "memory": 3008,
      "maxDuration": 60
    },
    "api/proxy.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
